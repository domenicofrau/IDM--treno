<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<joined-subclass
		name="com.idm.trenohibernate.FR.FRPasseggeriBusiness"
		extends="com.idm.trenohibernate.PasseggeriBusiness"
		table="FR_PASSEGGERI_BUSINESS">

		<key column="ID" />

		<property name="lunghezza" column="LUNGHEZZA" type="int" />
		<property name="peso" column="PESO" type="int" />
		<property name="prezzo" column="PREZZO" type="int" />
		<property name="postiNormali" column="P_NORMALI" type="int" />
		<property name="postiDisabili" column="P_DISABILI" type="int" />
		<property name="postiTotali" column="P_TOTALI" type="int" />
		<property name="servizi" column="SERVIZI" type="boolean" />

	</joined-subclass>

</hibernate-mapping>